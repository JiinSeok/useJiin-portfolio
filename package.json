{
  "name": "intellifam-landing-page",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "node ./server.js",
    "tailwindcss": "tailwindcss",
    "prepare": "husky install",
    "lint": "next lint --fix && stylelint '**/*.css' --fix",
    "format": "prettier --write .",
    "correct": "yarn format && yarn lint",
    "update-dev": "git fetch origin && git rebase origin/dev",
    "pre-push": "yarn correct && yarn build"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{css}": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": "./lint/.eslintrc.json"
  },
  "stylelint": {
    "extends": "./lint/.stylelintrc.json"
  },
  "prettier": "./lint/.prettierrc.json",
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-progress": "^1.1.6",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.11",
    "@tanstack/react-query": "^5.75.5",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "esbuild": "^0.25.4",
    "lucide-react": "^0.508.0",
    "next": "14.2.3",
    "next-intl": "^4.1.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.3",
    "react-markdown": "^10.1.0",
    "react-modal": "^3.16.3",
    "recharts": "^2.15.3",
    "remark-gfm": "^4.0.1",
    "sharp": "^0.34.1",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "zod": "^3.24.4",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@tailwindcss/postcss": "^4.1.5",
    "@types/babel__core": "^7.20.5",
    "@types/node": "^22.15.16",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@types/react-modal": "^3.16.3",
    "@types/supports-color": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.56.0",
    "eslint-config-next": "^15.3.2",
    "eslint-config-prettier": "^10.1.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^9.1.7",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "stylelint": "^16.19.1",
    "stylelint-config-tailwindcss": "^1.0.0",
    "tailwindcss": "^4.1.5",
